{% extends 'base.html.twig' %}

{% block title %}Project{% endblock %}

{% block body %}
    <h1>Project</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ project.id }}</td>
            </tr>
            <tr>
                <th>Filiere</th>
                <td>{{ project.filiere }}</td>
            </tr>
            <tr>
                <th>IsFinish</th>
                <td>{{ project.isFinish ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>IsActive</th>
                <td>{{ project.isActive ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_project_index') }}">back to list</a>

    <a href="{{ path('app_project_edit', {'id': project.id}) }}">edit</a>

    {{ include('project/_delete_form.html.twig') }}
{% endblock %}
